.container {
  padding: 20rpx;
}
text {
  display: block;
  margin-top: 10rpx;
  font-size: 28rpx;
}
input {
  width: 90%;
  margin: 10rpx 0;
  padding: 10rpx;
  border: 1px solid #ccc;
  border-radius: 8rpx;
}
button {
  margin-top: 20rpx;
  background-color: #007aff;
  color: white;
  padding: 10rpx;
  border: none;
  border-radius: 8rpx;
}
